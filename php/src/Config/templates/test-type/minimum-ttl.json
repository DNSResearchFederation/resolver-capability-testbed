{
  "type": "minimum-ttl",
  "description": "Minimum TTL Allowed",
  "config": {
    "dnsZone": {
      "records": [
        {
          "prefix": "*.ttl10",
          "ttl": 10,
          "type": "A",
          "data": "IPV4_ADDRESS"
        },
        {
          "prefix": "*.ttl15",
          "ttl": 15,
          "type": "A",
          "data": "IPV4_ADDRESS"
        }
      ]
    },
    "webVirtualHost": {
      "wildcard": true,
      "content": "OK",
      "sslCertPrefixes": [
        "*",
        "*.ttl10",
        "*.ttl15"
      ]
    }
  },
  "rules": {
    "dns": {
      "expectedQueries": [
        {
          "type": "A",
          "value": "(?i)^[a-z0-9-]{36}\\.ttl10\\.[a-z]+\\.[a-z]+"
        },
        {
          "type": "A",
          "value": "(?i)^[a-z0-9-]{36}\\.ttl10\\.[a-z]+\\.[a-z]+",
          "absent": true
        },
        {
          "type": "A",
          "value": "(?i)^[a-z0-9-]{36}\\.ttl15\\.[a-z]+\\.[a-z]+"
        },
        {
          "type": "A",
          "value": "(?i)^[a-z0-9-]{36}\\.ttl15\\.[a-z]+\\.[a-z]+",
          "absent": true
        }
      ]
    },
    "webserver": {
      "expectedQueries": 4
    },
    "webserverOptional": false,
    "relationalKey": "HOSTNAME",
    "timeoutSeconds": 15
  }
}