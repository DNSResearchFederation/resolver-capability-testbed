{
  "type": "dnssec-unvalidated",
  "description": "Test whether resolvers resolve records when a zone is signed using DNSSEC but not validated with DS records at the Registrar",
  "parameters": [
    {
      "identifier": "ALGORITHM"
    },
    {
      "identifier": "KEYSTRENGTH",
      "optional": true
    }
  ],
  "config": {
    "dnsZones": [
      {
        "dnsSecConfig": {
          "algorithm": "ALGORITHM",
          "keyStrength": "KEYSTRENGTH",
          "generateDSRecords": false
        },
        "records": [
          {
            "prefix": "*",
            "ttl": 200,
            "type": "A",
            "data": "IPV4_ADDRESS"
          }
        ]
      }
    ],
    "webVirtualHosts": [
      {
        "wildcard": true,
        "content": "OK",
        "sslCertPrefixes": [
          "*"
        ]
      }
    ]
  },
  "rules": {
    "dns": {
      "expectedQueries": [
        {
          "type": "A",
          "value": "(?i)^[a-z0-9-]{36}\\.[a-z]+\\.[a-z]+"
        }
      ]
    },
    "webserver": {
      "expectedQueries": 1
    },
    "webserverOptional": false,
    "relationalKey": "HOSTNAME",
    "timeoutSeconds": 3
  }
}