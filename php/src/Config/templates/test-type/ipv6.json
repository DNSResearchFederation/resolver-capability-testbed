{
  "type": "ipv6",
  "description": "IPv6 Records Defined Only for domain name",
  "config": {
    "dnsZones": [
      {
        "prefix": "ipv4-",
        "records": [
          {
            "prefix": "*",
            "ttl": 200,
            "type": "A",
            "data": "IPV4_ADDRESS"
          }
        ],
        "nameserverSet": "DEFAULT"
      },
      {
        "prefix": "",
        "records": [
          {
            "prefix": "*",
            "ttl": 200,
            "type": "A",
            "data": "IPV4_ADDRESS"
          }
        ],
        "nameserverSet": "NAMESERVERS_SET"
      }
    ],
    "webVirtualHosts": [
      {
        "wildcard": true,
        "content": "OK",
        "prefix": ""
      },
      {
        "prefix": "ipv4-",
        "wildcard": true,
        "content": "OK"
      }
    ]
  },
  "rules": {
    "dns": {
      "expectedQueries": [
        {
          "type": "A",
          "value": "(?i)^[a-z0-9-]{36}\\.[a-z]+\\.[a-z]+"
        },
        {
          "type": "A",
          "value": "(?i)^[a-z0-9-]{36}\\.ipv4-[a-z]+\\.[a-z]+"
        }
      ]
    },
    "webserver": {
      "expectedQueries": 2
    },
    "webserverOptional": true,
    "relationalKey": "HOSTNAME",
    "timeoutSeconds": 3
  }
}