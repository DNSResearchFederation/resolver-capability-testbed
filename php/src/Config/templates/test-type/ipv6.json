{
  "type": "ipv6",
  "description": "IPv6 Records Defined Only for domain name",
  "config": {
    "dnsZones": [
      {
        "prefix": "-ipv4",
        "records": [
          {
            "prefix": "*",
            "ttl": 200,
            "type": "A",
            "data": "IPV4_ADDRESS"
          }
        ],
        "nameservers": "DEFAULT"
      },
      {
        "prefix": "",
        "records": [
          {
            "prefix": "*",
            "ttl": 200,
            "type": "A",
            "data": "IPV4_ADDRESS"
          }
        ],
        "nameservers": "NAMESERVERS_SET"
      }
    ],
    "webVirtualHost": {
      "wildcard": true,
      "content": "OK",
      "prefix": ""
    }
  },
  "rules": {
    "dns": {
      "expectedQueries": [
        {
          "type": "A"
        },
        {
          "prefix": "-ipv4",
          "type": "A"
        }
      ]
    },
    "webserver": {
      "expectedQueries": 1
    },
    "webserverOptional": true,
    "relationalKey": "HOSTNAME_OR_IP",
    "timeoutSeconds": 3
  }
}