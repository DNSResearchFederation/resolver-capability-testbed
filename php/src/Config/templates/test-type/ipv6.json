{
  "type": "ipv6",
  "description": "IPv6 Records Defined Only for domain name",
  "config": {
    "dnsZone": {
      "records": [
      ]
    },
    "webVirtualHost": {
      "wildcard": true,
      "content": "OK"
    }
  },
  "rules": {
    "dns": {
      "expectedQueries": [
        {
          "type": "A"
        },
        {
          "type": "AAAA"
        }
      ]
    },
    "webserver": {
      "expectedQueries": 1
    },
    "webserverOptional": true,
    "relationalKey": "HOSTNAME",
    "timeoutSeconds": 3
  },
  "computedLogFields": {
    "status": "dns_response | and webserver_response | ternary 'Success' 'Failed'"
  }
}