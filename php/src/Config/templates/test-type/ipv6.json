{
  "type": "ipv6",
  "description": "IPv6 Records Defined Only for domain name",
  "config": {
    "dnsZone": {
      "records": [
        {
          "prefix": "*",
          "ttl": 200,
          "type": "A",
          "data": "IPV4_ADDRESS"
        }
      ]
    },
    "webVirtualHost": {
      "wildcard": true,
      "content": "OK"
    }
  },
  "rules": {
    "dns": {
      "expectedQueries": [
        {
          "type": "A"
        }
      ]
    },
    "webserver": {
      "expectedQueries": 1
    },
    "webserverOptional": true,
    "relationalKey": "HOSTNAME",
    "timeoutSeconds": 3
  }
}